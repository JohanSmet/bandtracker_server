{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "isolatedModules": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "js",
    "types": []
  },
  "exclude": [
    "node_modules",
    "obj",
    "bin"
  ],
  "filesGlob": [
    "./**/*.ts",
    "./**/*.tsx",
    "!./node_modules/**/*"
  ],
  "files": [
    "./background/TaskMusicBrainzArtists.ts",
    "./background/TaskMusicBrainzUrl.ts",
    "./background/TaskScheduler.ts",
    "./background/TaskSetlistFmTourDates.ts",
    "./background/TaskWikipediaBandBio.ts",
    "./background/TaskWikipediaTourDates.ts",
    "./background/TaskWikipediaTourList.ts",
    "./config/auth.ts",
    "./config/db.ts",
    "./config/http.ts",
    "./controller/AuthController.ts",
    "./controller/BandController.ts",
    "./controller/CityController.ts",
    "./controller/CountryController.ts",
    "./controller/TaskController.ts",
    "./controller/TourDateController.ts",
    "./controller/VenueController.ts",
    "./KeyValueStore.ts",
    "./middleware_auth.ts",
    "./model/Band.ts",
    "./model/Country.ts",
    "./model/IBandPOJO.ts",
    "./model/ICountryPOJO.ts",
    "./model/IKeyValuePOJO.ts",
    "./model/ITaskPOJO.ts",
    "./model/ITourDatePOJO.ts",
    "./model/IUserPOJO.ts",
    "./model/KeyValue.ts",
    "./model/Task.ts",
    "./model/TourDate.ts",
    "./model/User.ts",
    "./server.ts"
  ]
}
